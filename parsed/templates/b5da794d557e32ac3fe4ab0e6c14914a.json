{"components/chiara/common/footer-newsletter":"<article class=\"footer-info-col footer-info-col--newsletter\" data-section-type=\"newsletterSubscription\">\n    <div class=\"footer-newsletter-content\">\n        <h5 class=\"footer-info-heading\">{{lang 'newsletter.subscribe'}}</h5>\n        <p>{{lang 'newsletter.subscribe_intro'}}</p>\n        {{#if settings.show_newsletter_summary}}\n            <p>{{settings.newsletter_summary}}</p>\n        {{/if}}\n    </div>\n    <form class=\"form footer-newsletter-form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\n        <fieldset class=\"form-fieldset\">\n            <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n            <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n            <input type=\"hidden\" name=\"check\" value=\"1\">\n            <div class=\"form-field\">\n                <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\n                <div class=\"footer-newsletter-holder\">\n                    <input class=\"form-input footer-newsletter-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.email}}\" placeholder=\"{{lang 'newsletter.email_placeholder'}}\">\n                    <div class=\"button footer-newsletter-button\">\n                        <input type=\"submit\" value=\"{{lang 'newsletter.subscribe_submit'}}\">\n                    </div>\n                </div>\n            </div>\n        </fieldset>\n    </form>\n</article>\n"}
